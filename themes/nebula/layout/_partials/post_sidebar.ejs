<div id="post-sidebar" class="post-sidebar">
    <p>
        <%- partial("_partials/post_tags.ejs") %>
    </p>

    <p>
        <%- partial("_partials/post_categories.ejs") %>
    </p>

    <p>
        <h3>Outline</h3>
        <div id="post-sidebar--post-headers" class="post-sidebar--post-headers">
            <!-- Javascript injects post headers into here for quick navigation-->
        </div>
    </p>

    <p>
        <h3>Jump to</h3>
        <a href="javascript:window.scrollTo(0, 0);"><i class="icofont-arrow-up"></i> Top</a>
        <br>
        <a href="javascript:jumpTo('#share');"><i class="icofont-share-alt"></i> Share</a>
        <br>
        <a href="javascript:jumpTo('#comments');"><i class="icofont-comment"></i> Comments</a>
        <br>
        <a href="javascript:jumpTo('#footer');"><i class="icofont-info-circle"></i> Meta</a>
    </p>
</div>