<!doctype html>
<html lang="en">

<head>
    <%- partial("_partials/head.ejs") %>
</head>

<body>

    <div class="page-top">
        <%- partial("_partials/navbar.ejs") %>
    </div>

    <div class="page-center">
        <!-- Conditionals for other layouts -->
        <% if (page.layout === "download") { %>
            <%- partial("_partials/layouts/download.ejs") %>
        <% } else if (page.layout === "project") { %>
            <%- partial("_partials/layouts/project.ejs") %>
        <% } else if (page.layout === "project_index") { %>
            <%- partial("_partials/layouts/project_index.ejs") %>
        <% } else { %>
            <%- body %>
        <% } %>
    </div>

    <div class="page-bottom">
        <%- partial("_partials/scripts.ejs") %>
    </div>

</body>

</html>